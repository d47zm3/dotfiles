{
  "suggest.enablePreselect": false,
  "suggest.noselect": true,
  "languageserver": {
    "terraform": {
      "command": "terraform-lsp",
      "filetypes": ["terraform"],
      "initializationOptions": {}
    },
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"]
    }
  },
  "yaml.schemas": {
      "kubernetes": "/*.yaml"
  },
  "python.autoComplete.addBrackets": true
}
